@PACKAGE_INIT@

set(_QtExtraSpinBoxes_supported_components "")

if(@QtExtraSpinBoxes_BUILD_SHARED@)
    list(APPEND _QtExtraSpinBoxes_supported_components shared)
endif()

if(@QtExtraSpinBoxes_BUILD_STATIC@)
    list(APPEND _QtExtraSpinBoxes_supported_components static)
endif()

if(QtExtraSpinBoxes_FIND_COMPONENTS)
    foreach(_comp ${QtExtraSpinBoxes_FIND_COMPONENTS})
        if(NOT _comp IN_LIST _QtExtraSpinBoxes_supported_components)
            set(QtExtraSpinBoxes_FOUND False)
            set(QtExtraSpinBoxes_NOT_FOUND_MESSAGE "Unsupported component: ${_comp}")
        endif()

        include("${CMAKE_CURRENT_LIST_DIR}/QtExtraSpinBoxes-${_comp}.cmake")
    endforeach()
else()
    foreach(_component_config IN LISTS _QtExtraSpinBoxes_supported_components)
        include("${CMAKE_CURRENT_LIST_DIR}/QtExtraSpinBoxes-${_component_config}.cmake")
    endforeach()
endif()
